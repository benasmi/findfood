<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context="com.example.benas.findfood.ProfileActivity"
    android:background="#36f1c40f">

<ScrollView
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="0dp"
                android:id="@+id/layout"
                android:layout_weight="1">

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="200dp"
                    android:background="#8d312804"
                    android:id="@+id/background_picture"
                    android:layout_alignParentTop="true" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="60dp"
                    android:id="@+id/imageView123"
                    android:background="#000000"
                    android:alpha="0.3"
                    android:layout_alignTop="@+id/truck_name"
                    android:layout_alignParentStart="true" />

                <ImageView
                    android:layout_width="300dp"
                    android:layout_height="1dp"
                    android:id="@+id/imageView3"
                    android:background="#000000"
                    android:layout_alignBottom="@+id/truck_name"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="4dp"
                    android:id="@+id/imageView4"
                    android:background="#ca000000"
                    android:layout_below="@+id/background_picture"
                    android:layout_alignParentStart="true" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/truck_name"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="27dp"
                    android:editable="false"
                    android:background="#00ffffff"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:textSize="25dp"
                    android:textColor="#ffffff" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/slogan"
                    android:layout_below="@+id/truck_name"
                    android:layout_centerHorizontal="true"
                    android:editable="false"
                    android:background="#00ffffff"
                    android:gravity="center"
                    android:textColor="#ffffff" />

                <ImageView
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:id="@+id/report_button"
                    android:src="@drawable/report_button"
                    android:onClick="report_button"
                    android:alpha="0.9"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="5dp"
                    android:paddingBottom="2dp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentStart="true"></RelativeLayout>

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Description"
                    android:id="@+id/textView5"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:textStyle="bold" />

                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="1dp"
                    android:id="@+id/imageView6"
                    android:background="#9c8b8b8b"
                    android:layout_below="@+id/textView5"
                    android:layout_centerHorizontal="true" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/description"
                    android:gravity="center"
                    android:hint="Description isn't set"
                    android:editable="false"
                    android:layout_below="@+id/imageView6"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:textStyle="italic"
                    android:layout_marginTop="5dp" />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:id="@+id/imageView37"
                    android:src="@drawable/description_icon"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="5dp"
                    android:layout_alignBottom="@+id/textView5"
                    android:layout_toStartOf="@+id/textView5"
                    android:layout_marginTop="2dp" />

            </RelativeLayout>

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="4dp"
                android:id="@+id/imageView7"
                android:background="#ca000000"
                android:layout_below="@+id/background_picture"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"

                android:layout_marginTop="5dp"
                android:layout_weight="1">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Menu"
                    android:id="@+id/textView6"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:textStyle="bold" />

                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="1dp"
                    android:id="@+id/imageView14"
                    android:layout_below="@+id/textView6"
                    android:background="#9c8b8b8b"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_width="375dp"
                    android:layout_height="200dp"
                    android:id="@+id/menu_picture"
                    android:background="#34e67e22"
                    android:layout_below="@+id/textView6"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp" />

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:id="@+id/imageView36"
                    android:src="@drawable/menu_icon"
                    android:layout_marginLeft="7dp"
                    android:layout_marginRight="0dp"
                    android:layout_alignBottom="@+id/imageView14"
                    android:layout_toStartOf="@+id/textView6" />

            </RelativeLayout>

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="4dp"
                android:id="@+id/imageView16"
                android:background="#ca000000"
                android:layout_below="@+id/background_picture"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_weight="1"
                android:layout_gravity="center_horizontal">

                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="1dp"
                    android:id="@+id/imageView17"
                    android:background="#9c8b8b8b"
                    android:layout_below="@+id/textView7"
                    android:layout_centerHorizontal="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Special offers"
                    android:id="@+id/textView7"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/special_offers"
                    android:gravity="center"
                    android:hint="No special offers at the moment"
                    android:editable="false"
                    android:layout_below="@+id/imageView17"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:textStyle="italic"
                    android:layout_marginTop="5dp" />

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:id="@+id/imageView35"
                    android:src="@drawable/label_icon"
                    android:layout_marginLeft="5dp"
                    android:layout_alignBottom="@+id/textView7"
                    android:layout_toStartOf="@+id/textView7"
                    android:layout_marginRight="5dp"
                    android:layout_marginBottom="3dp" />

            </RelativeLayout>

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="4dp"
                android:id="@+id/imageView18"
                android:background="#ca000000"
                android:layout_below="@+id/background_picture"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="5dp"
                android:layout_weight="1">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Schedule"
                    android:id="@+id/textView8"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:textStyle="bold"
                    android:layout_gravity="center_horizontal" />

                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="1dp"
                    android:id="@+id/imageView19"
                    android:background="#9c8b8b8b"
                    android:layout_below="@+id/textView8"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_width="15dp"
                    android:layout_height="15dp"
                    android:id="@+id/imageView33"
                    android:src="@drawable/schedule_icon"
                    android:layout_marginLeft="5dp"
                    android:layout_alignBottom="@+id/textView8"
                    android:layout_toStartOf="@+id/textView8"
                    android:layout_marginBottom="5dp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/Pirmadienis"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"

                    android:id="@+id/Antradienis"
                    android:background="#43d35400"
                    android:layout_marginBottom="5dp">


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Tuesday"
                        android:id="@+id/textView9"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView20"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView9"
                        android:layout_centerHorizontal="true" />

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/tuesday"
                        android:layout_below="@+id/textView9"
                        android:layout_centerHorizontal="true"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center"
                        android:editable="false"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/textView8"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:id="@+id/Pirmadienis"
                    android:background="#34e67e22"
                    android:layout_marginBottom="5dp">

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/monday"
                        android:layout_below="@+id/textView10"
                        android:layout_centerHorizontal="true"
                        android:editable="false"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView22"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView10"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Monday"
                        android:id="@+id/textView10"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/Antradienis"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:id="@+id/Treciadienis"
                    android:layout_marginBottom="5dp"
                    android:background="#34e67e22">

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/wednesday"
                        android:layout_below="@+id/textView11"
                        android:editable="false"
                        android:layout_centerHorizontal="true"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView24"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView11"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Wednesday"
                        android:id="@+id/textView11"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/Treciadienis"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:id="@+id/Ketvirtadienis"
                    android:layout_marginBottom="5dp"
                    android:background="#43d35400">

                    <EditText
                        android:layout_width="fill_parent"
                        android:editable="false"
                        android:layout_height="wrap_content"
                        android:id="@+id/thursday"
                        android:layout_below="@+id/textView12"
                        android:layout_centerHorizontal="true"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView26"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView12"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Thursday"
                        android:id="@+id/textView12"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/Ketvirtadienis"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:id="@+id/Penktadienis"
                    android:layout_marginBottom="5dp"
                    android:background="#34e67e22">

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/friday"
                        android:layout_below="@+id/textView13"
                        android:editable="false"
                        android:layout_centerHorizontal="true"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView28"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView13"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Friday"
                        android:id="@+id/textView13"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/Penktadienis"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:id="@+id/Sestadienis"
                    android:layout_marginBottom="5dp"
                    android:background="#43d35400">

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:editable="false"
                        android:id="@+id/saturday"
                        android:layout_below="@+id/textView14"
                        android:layout_centerHorizontal="true"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView30"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView14"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Saturday"
                        android:id="@+id/textView14"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_below="@+id/Sestadienis"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:id="@+id/Sekmadienis"
                    android:background="#34e67e22"
                    android:layout_marginBottom="5dp">

                    <EditText
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/sunday"
                        android:editable="false"
                        android:layout_below="@+id/textView15"
                        android:layout_centerHorizontal="true"
                        android:background="#00ffffff"
                        android:hint="Not given"
                        android:gravity="center" />

                    <ImageView
                        android:layout_width="300dp"
                        android:layout_height="1dp"
                        android:id="@+id/imageView32"
                        android:background="#9c8b8b8b"
                        android:layout_below="@+id/textView15"
                        android:layout_centerHorizontal="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Sunday"
                        android:id="@+id/textView15"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:textSize="15dp" />

                </RelativeLayout>

            </RelativeLayout>

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="4dp"
                android:id="@+id/imageView34"
                android:background="#ca000000"
                android:layout_below="@+id/background_picture"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1" >

                <ImageView
                    android:layout_width="200dp"
                    android:layout_height="1dp"
                    android:id="@+id/imageView21"
                    android:background="#9c8b8b8b"
                    android:layout_below="@+id/textView16"
                    android:layout_centerHorizontal="true" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Contact information"
                    android:id="@+id/textView16"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:gravity="center"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/number"
                    android:gravity="left|center"
                    android:hint="No phone number"
                    android:layout_below="@+id/profile_mail"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:textStyle="italic"
                    android:editable="false"
                    android:layout_marginTop="15dp"
                    android:paddingStart="10dp"
                    android:layout_alignEnd="@+id/profile_mail"
                    android:layout_alignStart="@+id/profile_mail" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/profile_mail"
                    android:editable="false"
                    android:hint="E-mail"
                    android:background="#00ffffff"
                    android:textStyle="italic"
                    android:layout_gravity="center_horizontal"
                    android:layout_weight="1"
                    android:gravity="left"
                    android:paddingStart="10dp"
                    android:layout_alignBottom="@+id/imageView23"
                    android:layout_toEndOf="@+id/imageView23" />

                <EditText
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:editable="false"
                    android:id="@+id/website"
                    android:layout_below="@+id/number"
                    android:background="#00ffffff"
                    android:layout_centerVertical="true"
                    android:textStyle="italic"
                    android:layout_marginTop="15dp"
                    android:layout_gravity="center_horizontal"
                    android:layout_weight="1"
                    android:paddingStart="10dp"
                    android:gravity="left"
                    android:layout_alignStart="@+id/number"
                    android:hint="No website" />

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:id="@+id/imageView23"
                    android:src="@drawable/mail"
                    android:layout_below="@+id/imageView21"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="7dp"
                    android:layout_marginLeft="5dp" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:id="@+id/imageView25"
                    android:background="#9c8b8b8b"
                    android:layout_below="@+id/profile_mail"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dp" />

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:id="@+id/imageView27"
                    android:src="@drawable/phone_icon"
                    android:layout_alignTop="@+id/imageView25"
                    android:layout_alignParentStart="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp" />

                <ImageView
                    android:layout_width="fill_parent"
                    android:layout_height="1dp"
                    android:id="@+id/imageView29"
                    android:background="#9c8b8b8b"
                    android:layout_below="@+id/number"
                    android:layout_centerHorizontal="true" />

                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:id="@+id/imageView31"
                    android:src="@drawable/website_icon"
                    android:layout_marginLeft="5dp"
                    android:layout_alignBottom="@+id/website"
                    android:layout_toStartOf="@+id/website" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="50dp"
                android:layout_marginTop="5dp"
                android:layout_weight="1" >

            </RelativeLayout>

            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="4dp"
                android:id="@+id/imageView15"
                android:background="#ca000000"
                android:layout_below="@+id/background_picture"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:layout_weight="1" />

        </LinearLayout>

</ScrollView>

    <android.support.v7.widget.AppCompatButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LOCATE"
        android:id="@+id/locate"
        android:onClick="locate"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:background="#CC1D1D"
        android:alpha="0.85"
        android:textSize="21dp"
        android:textColor="#ffffff" />


</RelativeLayout>